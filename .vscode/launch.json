	"version": "0.2.0",
	"configurations": [
		{
			"name": "palhm config",
			"type": "python",
			"request": "launch",
			"cwd": "${workspaceFolder}/src",
			"program": "palhm.py",
			"args": [
				"-f",
				"conf/py-debug/palhm.jsonc",
				"config"
			],
			"console": "integratedTerminal",
			"justMyCode": true
		},
		{
			"name": "palhm run default",
			"type": "python",
			"request": "launch",
			"cwd": "${workspaceFolder}/src",
			"program": "palhm.py",
			"args": [
				"-f",
				"conf/py-debug/palhm.jsonc",
				"run"
			],
			"console": "integratedTerminal",
			"justMyCode": true
		}
	]
}
